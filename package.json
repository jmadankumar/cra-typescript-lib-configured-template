{
  "name": "cra-typescript-lib-configured-template",
  "version": "0.1.0",
  "description": "CRA Typescript template configured with react, redux, redux thunk ,React Router, material-ui, tailwindcss, styled-components, axios",
  "author": {
    "name": "Madan Kumar",
    "email": "kumarmadan.j@gmail.com",
    "url": "https://github.com/jmadankumar"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
